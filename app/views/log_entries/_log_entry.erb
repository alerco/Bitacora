    <div class="three columns">
      <%= log_entry.created_at.strftime("%B %d, %Y") %>
    </div>
    <div class="nine columns">
      <%= log_entry.description %>
    </div>
    <div class="four columns">
       actions
    </div>
  </li>
<% end -%>

